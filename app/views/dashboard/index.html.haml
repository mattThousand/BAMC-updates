.banner_image
  .wrap
    // %h2.banner_image_copy We supply the beats you need to be a great MC. Browse our beats below!
  %p.class= notice
  %p.class= alert

.top
  .wrap
    %h3.section_title My beats
  = render 'beats/track_row', :beats => @my_beats

.edge

.wrap
  %h3.section_title My tracks
  - if @tracks.any?
    %ul.tracks
    - @tracks.each do |track|
      %li.track_row
        .wrap
          %ul.track_list_items
            %li= track.name
            %li= track.beat.name
            %li=link_to '', track.track, class: 'track_preview'
            %li=link_to 'Share', '#'
  - else
    %p You don't have any tracks right now

